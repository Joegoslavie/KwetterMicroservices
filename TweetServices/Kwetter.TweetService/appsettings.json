{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "kwetter-tweet-db": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=kwetter-tweet-db;Integrated Security=True;"
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "ProducerConfiguration": {
    "Servers": "localhost:2181",
    "GroupId": "microservice:tweet"
  }
}
